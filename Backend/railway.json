{
  "$schema": "https://railway.app/schema.json",
  "name": "resume-ai-backend",
  "region": "europe-west4",
  "builder": "nixpacks",
  "buildCommand": "mvn clean package -DskipTests",
  "startCommand": "java -jar target/resume-ai-backend-0.0.1-SNAPSHOT.jar",
  "deploy": {
    "healthcheckPath": "/",
    "healthcheckInterval": 10,
    "restartPolicy": "always",
    "minInstances": 1,
    "maxInstances": 1
  }
}
