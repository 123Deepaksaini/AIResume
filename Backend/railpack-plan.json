{
  "name": "resume-ai-backend",
  "region": "europe-west4",
  "builder": {
    "type": "docker",
    "dockerfile": "Dockerfile"
  },
  "deploy": {
    "startCommand": "java -jar target/resume-ai-backend-0.0.1-SNAPSHOT.jar",
    "healthcheckPath": "/actuator/health",
    "healthcheckInterval": 10,
    "restartPolicy": "always",
    "minInstances": 1,
    "maxInstances": 1
  }
}
